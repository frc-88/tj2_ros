{
  "name": "tj2_ros_devcontainer",
  "service": "tj2_ros_devcontainer",
  "dockerComposeFile": ["docker-compose.yaml", "docker-compose.gpu.yaml"],
  "initializeCommand": ".devcontainer/setup_container.sh",
  "workspaceFolder": "/root/tj2_ros",
  "shutdownAction": "stopCompose",
  "postCreateCommand": ".devcontainer/post_create.sh",
  "extensions": [
    "editorconfig.editorconfig",
    "ms-python.python",
    "ms-python.vscode-pylance",
    "ms-vscode.cpptools",
    "jtr.vscode-position",
    "dbaeumer.vscode-eslint",
    "bierner.folder-source-actions",
    "mhutchie.git-graph",
    "eamodio.gitlens",
    "ms-iot.vscode-ros",
    "visualstudioexptteam.vscodeintellicode",
    "visualstudioexptteam.intellicode-api-usage-examples",
    "ms-vscode-remote.vscode-remote-extensionpack",
    "ms-vsliveshare.vsliveshare",
    "esbenp.prettier-vscode",
    "styled-components.vscode-styled-components",
    "redhat.vscode-yaml",
    "ms-toolsai.jupyter-keymap",
    "ms-vscode-remote.remote-containers",
    "alpha4.jsonl",
    "hashicorp.terraform",
    "zchrissirhcz.cmake-highlight"
  ]
}
